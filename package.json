  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "test[.](js|ts)",
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "./src/**"
    ]
  }
