    "clean": "rimraf .cache && rimraf coverage && rimraf dist && rimraf node_modules",
    "prebuild": "rimraf dist",
    "start": "webpack serve --open --config webpack.development.config.js",
    "build": "webpack --config webpack.production.config.js",
